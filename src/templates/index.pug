    section.section.section_main
      .container.container_main
        header.header
          .header__contacts.contacts
            ul.contacts__info.contacts-info
              li.contacts-info__item
                i.fas.fa-phone.contacts-info__icon
                a(href="tel:8(800)555-35-35").contacts-info__text 8 (800) 555-35-35
              li.contacts-info__item
                .contacts-info__icon
                  i.fa.fa-envelope
                a(href="mailto:sales@artislunch.com").contacts-info__text sales@artislunch.com
            ul.contacts__socials.socials
              li.socials__item
                a(href="https://vk.com/artislunch").socials__link
