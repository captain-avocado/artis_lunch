          .header__row
            a(href="index.html").logo.header__logo
              img(src="images/logo.png" alt="").logo__img
            ul.menu.header__menu
              li.menu__item(data-scroll="1") 
                a(href="#").menu__link Преимущества
              li.menu__item(data-scroll="3") 
                a(href="#").menu__link Меню
              li.menu__item(data-scroll="4") 
                a(href="#").menu__link Контакты
              li.menu__item(data-scroll="5") 
