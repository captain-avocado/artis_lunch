    section.section.section_form
      .container.container_form
        form.form#form
          .form__left-col
            //- img(src="images/form.jpg" alt="").form__img
          .form__right-col
            h3.form__title Форма обратной связи
            ul.form__inputs.inputs
              li.inputs__item
                input(placeholder="Ваше имя" name="name").input.input_name
                i.fas.fa-pencil-alt.inputs__icon
              li.inputs__item
